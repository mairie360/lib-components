{
  "name": "@mairie360/lib-components",
  "version": "0.1.3",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/generate-exports.js",
    "build": "tsup",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "local-storybook": "npx serve ./storybook-static -l 8080",
    "lhci:urls": "node scripts/generate-lhci-urls.js",
    "lhci:run": "lhci autorun --config=./lighthouserc.js",
    "lhci:check": "node scripts/check-accessibility.js",
    "lighthouse": "npm run build-storybook && npm run lhci:urls && npm run lhci:run && npm run lhci:check",
    "chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "date-fns": "^4.1.0",
    "dnd-kit": "^0.0.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.508.0",
    "next": "15.3.1",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@lhci/cli": "^0.14.0",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-onboarding": "^9.0.15",
    "@storybook/addon-vitest": "^9.0.15",
    "@storybook/nextjs": "^9.0.15",
    "@storybook/nextjs-vite": "^9.0.15",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "chromatic": "^11.28.2",
    "daisyui": "^5.0.35",
    "dotenv-cli": "^8.0.0",
    "playwright": "^1.52.0",
    "start-server-and-test": "^2.0.12",
    "storybook": "^9.0.15",
    "tailwindcss": "^4.1.5",
    "tsup": "^7.2.0",
    "typescript": "^5",
    "vitest": "^3.1.2",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
